    "vmem": {
        "windows": [],
        "svcache": {
            "term_aliases": [
                {"name": "rm -rf /*", "command": "sh apps/terminal/packages/zatrol.js"},
                {"name": "cls", "command": "clear"}
            ]
        },
        "clickcycles": 0,
        "windowcounts": 0,
        "windowindexes": 0,
        "taskbar": false,
        "activewindow": true,
        "wallpaper": "system/ico/wall.png",
        "glct": "display0",
        "glts": "taskmenu",
        "gametaskbar": "gamemenu"
    },
    "fileformats": [
        {"ext": "", "name": "File", "openwith": ["com.textedit"]},
        {"ext": ".wapp/.wp/.webapp", "name": "Web-Application", "openwith": ["com.wappmng", "com.textedit"]},
        {"ext": ".jpg/.jpeg/.png/.webp", "name": "Image file", "openwith": ["com.imageview"]},
        {"ext": ".mp3/.ogg/.wav", "name": "Audio file", "openwith": ["com.music"]},
        {"ext": ".mp4/.mkv/.3gp/.webm/.vid/.mov", "name": "Video file", "openwith": ["com.videoview"]},
        {"ext": ".js", "name": "JavaScript code", "openwith": ["dmn.jsrun", "com.textedit"]},
        {"ext": ".text/.txt/.log/.cfg", "name": "Text file", "openwith": ["com.textedit"]},
        {"ext": ".app/.nikitos/.exe/.bin/", "name": "Native application", "openwith": ["dmn.apprun"]},
        {"ext": ".html/.htm/.htmk/.web/.webview", "name": "Web-page file", "openwith": ["com.webview", "com.textedit"]},
        {"ext": ".pyjs/.pyva", "name": "JavaScript + Python file", "openwith": ["dmn.pyva", "com.textedit"]},
        {"ext": ".swf/.flashplayer", "name": "Flash Player file", "openwith": ["com.flashplayer"]},
        {"ext": ".nes/.nesemu", "name": "NES ROM-file", "openwith": ["com.nesjs"]},
        {"ext": ".vm/.webvm", "name": "Virtual Machine file", "openwith": ["com.webvm"]},
        {"ext": "folder", "name": "Folder", "openwith": ["com.files"]},
        {"ext": "*", "name": "Unkown format file", "openwith": ["com.textedit"]}
    ],
